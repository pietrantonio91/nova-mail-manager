{
    "name": "pietrantonio/nova-mail-manager",
    "type": "library",
    "require": {
        "laravel/framework": "^10",
        "laravel/nova": "^4",
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "Pietrantonio\\NovaMailManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alessandro Pietrantonio",
            "email": "pietrantonio.alessandro@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Pietrantonio\\NovaMailManager\\NovaMailManagerServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
